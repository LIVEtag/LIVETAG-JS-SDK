{
  "name": "livetag-js-sdk",
  "version": "0.0.1",
  "license": "proprietary",
  "description": "Livetag JS SDK",
  "scripts": {
    "build": "NODE_ENV=development rollup -c",
    "dist": "NODE_ENV=production rollup -c",
    "dev": "NODE_ENV=development rollup -c -w",
    "prettier": "prettier --write \"./src/**/*.{ts,js,json}\"",
    "prettier:check": "prettier --list-different \"./src/**/*.{ts,js,json}\""
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-typescript": "^8.1.0",
    "@types/uuid": "^8.3.0",
    "husky": "^4.3.7",
    "lint-staged": "^10.5.3",
    "postcss": "^8.2.4",
    "prettier": "^2.2.1",
    "prettier-plugin-organize-imports": "^1.1.1",
    "rollup": "^2.36.1",
    "rollup-plugin-license": "^2.2.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.32.1",
    "ts-node": "^9.1.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3",
    "uuid": "^8.3.2"
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*{.ts,.js,.json}": [
      "prettier --write"
    ]
  }
}
