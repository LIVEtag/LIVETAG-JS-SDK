{
  "name": "livetag-sky21-js-sdk",
  "version": "1.0.0",
  "license": "proprietary",
  "description": "Livetag JS SDK",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-typescript": "^8.1.0",
    "husky": "^4.3.7",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "prettier-plugin-organize-imports": "^1.1.1",
    "rollup": "^2.36.1",
    "rollup-plugin-license": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "^9.1.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "build": "NODE_ENV=development rollup -c",
    "dist": "NODE_ENV=production rollup -c",
    "dev": "NODE_ENV=development rollup -c -w",
    "prettier": "prettier --write \"./src/**/*.{ts,js,json}\"",
    "prettier:check": "prettier --list-different \"./src/**/*.{ts,js,json}\""
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*{.ts,.js,.json}": [
      "prettier --write"
    ]
  }
}
