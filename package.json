{
  "name": "livetag-js-sdk",
  "version": "0.0.7",
  "license": "proprietary",
  "description": "Livetag JS SDK",
  "scripts": {
    "build": "cross-env NODE_ENV=development rollup -c && npm run snippet",
    "dist": "cross-env NODE_ENV=production rollup -c && npm run snippet",
    "dev": "cross-env NODE_ENV=development rollup -c -w",
    "snippet": "cross-env NODE_ENV=production node snippet-generate.js",
    "prettier": "prettier --write \"./src/**/*.{js,json,svelte}\"",
    "prettier:check": "prettier --list-different \"./src/**/*.{js,json,svelte}\"",
    "bump-version": "node utils/bump-version.js",
    "write-version": "node utils/write-version.js src/environments/version.ts"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.4.1",
    "@types/uuid": "^8.3.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "postcss": "^8.2.8",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "rollup": "^2.41.4",
    "rollup-plugin-license": "^2.3.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.35.0",
    "uuid": "^8.3.2"
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*{.js,.json,.svelte}": [
      "prettier --write"
    ]
  }
}